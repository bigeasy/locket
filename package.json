{
    "name":                 "locket",
    "version":              "1.0.0",
    "description":

      "A pure-JavaScript LevelDB implementation backed by a durable and persistent evented I/O b-tree for use with LevelUP.",

    "keywords":
    [
                            "btree", "leveldb", "levelup", "binary", "database",
                            "json", "b-tree", "concurrent", "persistence"
    ],
    "author":               "Alan Gutierrez <alan@prettyrobots.com>",
    "homepage":             "https://github.com/bigeasy/locket",
    "bugs":                 "https://github.com/bigeasy/locket/issues",
    "license":              "MIT",
    "repository":
    {
                            "type": "git",
                            "url": "http://github.com/bigeasy/locket.git"
    },
    "dependencies":
    {
                            "abstract-leveldown":           "2.6.x",
                            "b-tree":                       "1.0.x",
                            "cadence":                      "1.0.x",
                            "constrain":                    "1.0.x",
                            "mkdirp":                       "0.5.1",
                            "mvcc":                         "1.0.x",
                            "pair":                         "1.0.x",
                            "reactor":                      "2.2.x",
                            "rimraf":                       "2.5.4",
                            "sequester":                    "1.0.x",
                            "vestibule":                    "1.3.x"
    },
    "devDependencies":
    {
                            "arguable":                     "5.0.x",
                            "delta":                        "1.1.x",
                            "leveldown":                    "1.4.6",
                            "levelup":                      "1.3.2",
                            "proof":                        "1.2.x",
                            "seedrandom":                   "2.4.2",
                            "tap":                          "6.3.0"
    },
    "main":                 "locket",
    "scripts":
    {
                            "test": "proof platform win32 && proof test */*/*.t.js || t/test"
    }
}
